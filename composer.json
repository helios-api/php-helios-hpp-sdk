{
  "name": "helios-api/helios-hpp-sdk",
  "description": "Helios HPP payment system PHP SDK",
  "keywords": ["helios", "sdk"],
  "homepage": "https://github.com/helios-api/php-helios-hpp-sdk",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Dawid Szymczak",
      "email": "szymczak.dave@gmail.com"
    }
  ],
  "require": {
    "php": "^5.6|^7.0",
    "guzzlehttp/guzzle": "~6.0",
    "jms/serializer": "^1.4",
    "doctrine/common": "^2.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.5",
    "mockery/mockery": "~0.8",
    "squizlabs/php_codesniffer": "*",
    "phpunit/php-code-coverage": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "HeliosHpp\\": "src/HeliosHpp/",
      "HeliosApi\\": "src/HeliosApi/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "HeliosHpp\\Tests\\": "tests/HeliosHpp/",
      "HeliosApi\\Tests\\": "tests/HeliosApi/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.0.x-dev"
    }
  }
}
